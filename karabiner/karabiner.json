{
  "version": 3,
  "profiles": [
    {
      "name": "Default",
      "virtual_hid_keyboard": {
        "keyboard_type_v2": "ansi"
      },
      "rules": [
        {
          "description": "Homerow mods (left hand)",
          "manipulators": [
            {
              "type": "basic",
              "from": { "key_code": "a" },
              "to_if_held_down": [{ "key_code": "left_shift" }],
              "to_if_alone": [{ "key_code": "a" }],
              "parameters": { "basic.hold_down_milliseconds": 100 }
            },
            {
              "type": "basic",
              "from": { "key_code": "s" },
